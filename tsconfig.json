{
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["vite/client", "vite-plugin-svgr/client"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src"
  }
}
